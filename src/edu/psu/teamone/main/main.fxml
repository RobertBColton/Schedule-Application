<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="edu.psu.teamone.main.Controller">
  <center>
    <TabPane fx:id="tabPane">
      <Tab closable="false" text="%sectionsTab">
        <TableView>
          <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" fx:id="x1" />
          </columnResizePolicy>
          <columns>
            <TableColumn text="%id" />
            <TableColumn text="%name" />
            <TableColumn text="%abbreviation" />
            <TableColumn text="%number" />
            <TableColumn text="%bulletinURI" />
          </columns>
        </TableView>
      </Tab>
      <Tab closable="false" text="%instructorsTab">
        <TableView columnResizePolicy="$x1">
          <columns>
            <TableColumn text="%id" />
            <TableColumn text="%name" />
          </columns>
        </TableView>
      </Tab>
      <Tab closable="false" text="%schedulesTab">
        <TableView columnResizePolicy="$x1">
          <columns>
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="373.0" text="%section" />
            <TableColumn text="%meeting" />
          </columns>
        </TableView>
      </Tab>
      <Tab closable="false" text="%AddSectionTab">
        <content>
          <Pane prefHeight="295.0" prefWidth="433.0">
            <children>
              <GridPane id="GridPane" layoutX="89.0" layoutY="59.0">
                <children>
                  <AnchorPane id="AnchorPane" minHeight="45.0" prefHeight="45.0" GridPane.columnIndex="0" GridPane.rowIndex="5">
                    <children>
                      <HBox layoutX="85.0" layoutY="3.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="22.0" prefWidth="233.0" spacing="21.0">
                        <children>
                          <CheckBox fx:id="dayTues" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="T" HBox.hgrow="ALWAYS" />
                          <CheckBox fx:id="dayWed" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="W" HBox.hgrow="ALWAYS" />
                          <CheckBox fx:id="dayThur" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="21.9998779296875" prefWidth="29.0" text="R" HBox.hgrow="ALWAYS" />
                          <CheckBox fx:id="dayFri" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="F" HBox.hgrow="ALWAYS" />
                        </children>
                      </HBox>
                      <VBox id="VBox" alignment="CENTER" layoutX="114.0" layoutY="132.0" spacing="5.0" />
                      <CheckBox fx:id="dayMon" layoutX="24.0" layoutY="6.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="M" />
                    </children>
                  </AnchorPane>
                  <TextField fx:id="sectionName" prefWidth="618.0" promptText="Name:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                  <TextField fx:id="sectionAbb" prefWidth="618.0" promptText="Abbreviation:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                  <TextField fx:id="sectionId" prefWidth="618.0" promptText="Number:" GridPane.columnIndex="0" GridPane.rowIndex="2" />
                  <TextField fx:id="startTime" prefWidth="618.0" promptText="Start Time: 00:00" GridPane.columnIndex="0" GridPane.rowIndex="3" />
                  <TextField fx:id="endTime" prefWidth="618.0" promptText="EndTime: 00:00" GridPane.columnIndex="0" GridPane.rowIndex="4" />
                  <Button id="Add" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#addSectionAction" prefHeight="22.0" prefWidth="618.0" text="%addSection" wrapText="false" GridPane.columnIndex="0" GridPane.rowIndex="6" />
                </children>
                <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                </columnConstraints>
                <rowConstraints>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                </rowConstraints>
              </GridPane>
            </children>
          </Pane>
        </content>
      </Tab>
    </TabPane>
  </center>
  <top>
    <VBox id="vbox">
      <MenuBar fx:id="menuBar">
        <menus>
          <Menu text="%fileButton">
            <items>
              <MenuItem text="%newButton" />
              <MenuItem text="%openButton" fx:id="openButton" />
              <MenuItem text="%saveButton" />
              <MenuItem text="%saveAsButton" />
              <SeparatorMenuItem />
              <MenuItem onAction="#handleExitAction" text="%exitButton" />
            </items>
          </Menu>
          <Menu text="%editButton">
            <items>
              <MenuItem text="%cutButton" />
              <MenuItem text="%copyButton" />
              <MenuItem text="%pasteButton" />
            </items>
          </Menu>
          <Menu text="%helpButton">
            <items>
              <MenuItem onAction="#handleDocAction" text="%documentationButton" />
              <MenuItem onAction="#handleWebAction" text="%websiteButton" />
              <SeparatorMenuItem />
              <MenuItem onAction="#handleAboutAction" text="%aboutButton" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <ToolBar>
        <Button fx:id="openButton" text="%openButton" />
        <Button fx:id="saveButton" text="%saveButton" />
      </ToolBar>
    </VBox>
  </top>
</BorderPane>